<!DOCTYPE html>
<html lang="en">

<head>
    
    <title>About </title>
</head>

<body>
    <%- include('../components/header'); %>

    <h1>Welcome to the about page</h1>
    <h3>
        <%= user.name %>
    </h3>
    <h3>
        <%= user.fname %>
    </h3>
    <h3>
        <%= user.email %>
    </h3>
    <h3>
        <%= user.country %>
    </h3>
    <ul>
        <% user.skills.forEach((item) => { %>
            <li>
                <%= item %>
            </li>
        <% }); %>
    </ul>
</body>


</html>